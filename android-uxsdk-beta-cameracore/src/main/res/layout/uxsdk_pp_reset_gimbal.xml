<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/uxsdk_30_dp"
    android:layout_height="wrap_content"
    android:background="@drawable/uxsdk_bg_white_box"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <ImageView
        android:id="@+id/imgGimbalResetCenter"
        android:layout_width="@dimen/uxsdk_30_dp"
        android:layout_height="@dimen/uxsdk_30_dp"
        android:src="@drawable/ic_gimbal_reset_center" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/uxsdk_white_47_percent" />


    <ImageView
        android:id="@+id/imgGimbalResetCenterYaw"
        android:layout_width="@dimen/uxsdk_30_dp"
        android:layout_height="@dimen/uxsdk_30_dp"
        android:src="@drawable/ic_gimbal_reset_center_yaw" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/uxsdk_white_47_percent" />

    <ImageView
        android:id="@+id/imgGimbalResetPitchDown"
        android:layout_width="@dimen/uxsdk_30_dp"
        android:layout_height="@dimen/uxsdk_30_dp"
        android:src="@drawable/ic_gimbal_reset_pitch_down" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/uxsdk_white_47_percent" />

    <ImageView
        android:id="@+id/imgGimbalResetYawPitchDown"
        android:layout_width="@dimen/uxsdk_30_dp"
        android:layout_height="@dimen/uxsdk_30_dp"
        android:src="@drawable/ic_gimbal_reset_yaw_pitch_down" />


</LinearLayout>